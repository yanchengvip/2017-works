<table class="table table-striped table-hover" id="chosed_product_table">
  <thead>
    <tr>
      <th>重宝编号</th>
      <th width="60%">重宝名称</th>
    </tr>
  </thead>
  <tbody id="chosed_product_body">

    <% @products && @products.each do |product| %>
      <tr class="fields content_tr">
        <td><%= product.sku %></td>
        <td><%= product.name %></td>
      </tr>
    <% end %>

  </tbody>
</table>
