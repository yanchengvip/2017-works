<div class="row placeholders well">
  <%= form_for msg_records_path, html: {method: :get, class: "navbar-form navbar-left text-left", id: 'search_form'}  do |f| %>
    <div class="form-group row">
      <span>创建时间：</span>
      <%= text_field_tag "q[created_at_gteq]", params[:q][:created_at_gteq], class: "form-control mr_30", placeholder: "创建时间" %>
      <%= hidden_field_tag "msg_type", 2 %>
    </div>

    <%= f.submit '搜索', class: "btn btn-primary mr_10" %>
    <!-- <button type="reset" class="btn btn-default" id="reset_form">重置</button> -->
  <% end %>
</div>
