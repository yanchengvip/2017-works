<p id="notice"><%= notice %></p>

<h1>Mammon User Codes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Period</th>
      <th>Codes</th>
      <th>Count</th>
      <th>Attack user</th>
      <th>Source type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mammon_user_codes.each do |mammon_user_code| %>
      <tr>
        <td><%= mammon_user_code.user_id %></td>
        <td><%= mammon_user_code.period_id %></td>
        <td><%= mammon_user_code.codes %></td>
        <td><%= mammon_user_code.count %></td>
        <td><%= mammon_user_code.attack_user_id %></td>
        <td><%= mammon_user_code.source_type %></td>
        <td><%= link_to 'Show', mammon_user_code %></td>
        <td><%= link_to 'Edit', edit_mammon_user_code_path(mammon_user_code) %></td>
        <td><%= link_to 'Destroy', mammon_user_code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mammon User Code', new_mammon_user_code_path %>
