<div class="row">
  <p class="sub-header">系统管理>其它管理>操作日志</p>
</div>

<div class="row">
  <table class="table table-striped table-hover">
    <thead>
    <tr>
      <th>操作人员ID</th>
      <th>操作人手机号</th>
      <th>操作资源</th>
      <th>资源ID</th>
      <th>处理方式</th>
      <th>操作详情</th>
      <th>操作时间</th>
    </tr>
    </thead>
    <tbody>
    <% @auditeds && @auditeds.each do |aud| %>
        <tr>
          <td><%= aud.user_id%></td>
          <td><%= aud.user.phone%></td>
          <td><%= aud.auditable_type%></td>
          <td><%= aud.auditable_id%></td>
          <td><%= aud.action%></td>
          <td><%= aud.audited_changes%></td>
          <td><%= aud.created_at.strftime('%Y-%m-%d %H:%M')%></td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>

<%= paginate @auditeds %>