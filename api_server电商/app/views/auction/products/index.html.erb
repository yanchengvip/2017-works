<p id="notice"><%= notice %></p>

<h1>Auction Products</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auction_products.each do |auction_product| %>
      <tr>
        <td><%= link_to 'Show', auction_product %></td>
        <td><%= link_to 'Edit', edit_auction_product_path(auction_product) %></td>
        <td><%= link_to 'Destroy', auction_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auction Product', new_auction_product_path %>
