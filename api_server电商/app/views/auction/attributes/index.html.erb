<p id="notice"><%= notice %></p>

<h1>Auction Attributes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auction_attributes.each do |auction_attribute| %>
      <tr>
        <td><%= link_to 'Show', auction_attribute %></td>
        <td><%= link_to 'Edit', edit_auction_attribute_path(auction_attribute) %></td>
        <td><%= link_to 'Destroy', auction_attribute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auction Attribute', new_auction_attribute_path %>
