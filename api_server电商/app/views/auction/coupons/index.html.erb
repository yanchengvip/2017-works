<p id="notice"><%= notice %></p>

<h1>Auction Coupons</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auction_coupons.each do |auction_coupon| %>
      <tr>
        <td><%= link_to 'Show', auction_coupon %></td>
        <td><%= link_to 'Edit', edit_auction_coupon_path(auction_coupon) %></td>
        <td><%= link_to 'Destroy', auction_coupon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auction Coupon', new_auction_coupon_path %>
