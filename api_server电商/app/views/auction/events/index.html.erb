<p id="notice"><%= notice %></p>

<h1>Auction Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auction_events.each do |auction_event| %>
      <tr>
        <td><%= link_to 'Show', auction_event %></td>
        <td><%= link_to 'Edit', edit_auction_event_path(auction_event) %></td>
        <td><%= link_to 'Destroy', auction_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auction Event', new_auction_event_path %>
