<p id="notice"><%= notice %></p>

<h1>Auction Brands</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auction_brands.each do |auction_brand| %>
      <tr>
        <td><%= link_to 'Show', auction_brand %></td>
        <td><%= link_to 'Edit', edit_auction_brand_path(auction_brand) %></td>
        <td><%= link_to 'Destroy', auction_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auction Brand', new_auction_brand_path %>
