<p id="notice"><%= notice %></p>

<h1>Auction Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auction_users.each do |auction_user| %>
      <tr>
        <td><%= link_to 'Show', auction_user %></td>
        <td><%= link_to 'Edit', edit_auction_user_path(auction_user) %></td>
        <td><%= link_to 'Destroy', auction_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auction User', new_auction_user_path %>
