<p id="notice"><%= notice %></p>

<h1>Core Logins</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @core_logins.each do |core_login| %>
      <tr>
        <td><%= link_to 'Show', core_login %></td>
        <td><%= link_to 'Edit', edit_core_login_path(core_login) %></td>
        <td><%= link_to 'Destroy', core_login, method: :delete, data: { confirm: '
Are you sure?

你确定吗？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

