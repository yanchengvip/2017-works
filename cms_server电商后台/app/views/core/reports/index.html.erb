<h1>Core Reports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @core_reports.each do |core_report| %>
      <tr>
        <td><%= link_to 'Show', core_report %></td>
