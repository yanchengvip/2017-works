<%= simple_form_for(@supplier_provider) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :active %>
    <%= f.input :login %>
    <%= f.input :password %>
    <%= f.input :salt %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
