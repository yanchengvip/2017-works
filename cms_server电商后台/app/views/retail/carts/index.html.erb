<p id="notice"><%= notice %></p>

<h1>Retail Carts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @retail_carts.each do |retail_cart| %>
      <tr>
        <td><%= link_to 'Show', retail_cart %></td>
        <td><%= link_to 'Edit', edit_retail_cart_path(retail_cart) %></td>
        <td><%= link_to 'Destroy', retail_cart, method: :delete, data: { confirm: '
Are you sure?

你确定吗？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Retail Cart', new_retail_cart_path %>
