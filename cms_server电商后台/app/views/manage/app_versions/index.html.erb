<p id="notice"><%= notice %></p>

<h1>Manage App Versions</h1>

<table>
  <thead>
    <tr>
      <th>App type</th>
      <th>App version</th>
      <th>Name</th>
      <th>Mode</th>
      <th>Version code</th>
      <th>Url</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manage_app_versions.each do |manage_app_version| %>
      <tr>
        <td><%= manage_app_version.app_type %></td>
        <td><%= manage_app_version.app_version %></td>
        <td><%= manage_app_version.name %></td>
        <td><%= manage_app_version.mode %></td>
        <td><%= manage_app_version.version_code %></td>
        <td><%= manage_app_version.url %></td>
        <td><%= manage_app_version.active %></td>
        <td><%= link_to 'Show', manage_app_version %></td>
        <td><%= link_to 'Edit', edit_manage_app_version_path(manage_app_version) %></td>
        <td><%= link_to 'Destroy', manage_app_version, method: :delete, data: { confirm: '
Are you sure?

你确定吗？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manage App Version', new_manage_app_version_path %>
