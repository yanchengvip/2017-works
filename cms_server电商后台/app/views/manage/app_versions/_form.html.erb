<%= simple_form_for(@manage_app_version) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :app_type %>
    <%= f.input :app_version %>
    <%= f.input :name %>
    <%= f.input :mode %>
    <%= f.input :version_code %>
    <%= f.input :url %>
    <%= f.input :active %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
