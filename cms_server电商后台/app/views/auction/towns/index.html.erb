<p id="notice"><%= notice %></p>

<h1>Auction Towns</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auction_towns.each do |auction_town| %>
      <tr>
        <td><%= link_to 'Show', auction_town %></td>
        <td><%= link_to 'Edit', edit_auction_town_path(auction_town) %></td>
        <td><%= link_to 'Destroy', auction_town, method: :delete, data: { confirm: '
Are you sure?

你确定吗？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

