<%= simple_form_for(@auction_promotion_activity_apply_product) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :promotion_activity_id %>
    <%= f.input :provider_id %>
    <%= f.input :active %>
    <%= f.input :promotion_activity_apply_id %>
    <%= f.input :product_id %>
    <%= f.input :provider_price %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
