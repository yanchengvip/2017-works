<p id="notice"><%= notice %></p>

<h1>Auction Cards</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auction_cards.each do |auction_card| %>
      <tr>
        <td><%= link_to 'Show', auction_card %></td>
        <td><%= link_to 'Edit', edit_auction_card_path(auction_card) %></td>
        <td><%= link_to 'Destroy', auction_card, method: :delete, data: { confirm: '
Are you sure?

你确定吗？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

