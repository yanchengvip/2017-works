<p id="notice"><%= notice %></p>

<h1>Auction Cities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auction_cities.each do |auction_city| %>
      <tr>
        <td><%= link_to 'Show', auction_city %></td>
        <td><%= link_to 'Edit', edit_auction_city_path(auction_city) %></td>
        <td><%= link_to 'Destroy', auction_city, method: :delete, data: { confirm: '
Are you sure?

你确定吗？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auction City', new_auction_city_path %>
