<p id="notice"><%= notice %></p>

<h1>Exchange Product Price Notices</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Exchange product</th>
      <th>Current price</th>
      <th>Up price</th>
      <th>Down price</th>
      <th>Up price end time</th>
      <th>Down price end time</th>
      <th>Active</th>
      <th>Up notice</th>
      <th>Down notice</th>
      <th>Notice type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exchange_product_price_notices.each do |exchange_product_price_notice| %>
      <tr>
        <td><%= exchange_product_price_notice.user_id %></td>
        <td><%= exchange_product_price_notice.exchange_product_id %></td>
        <td><%= exchange_product_price_notice.current_price %></td>
        <td><%= exchange_product_price_notice.up_price %></td>
        <td><%= exchange_product_price_notice.down_price %></td>
        <td><%= exchange_product_price_notice.up_price_end_time %></td>
        <td><%= exchange_product_price_notice.down_price_end_time %></td>
        <td><%= exchange_product_price_notice.active %></td>
        <td><%= exchange_product_price_notice.up_notice %></td>
        <td><%= exchange_product_price_notice.down_notice %></td>
        <td><%= exchange_product_price_notice.notice_type %></td>
        <td><%= link_to 'Show', exchange_product_price_notice %></td>
        <td><%= link_to 'Edit', edit_exchange_product_price_notice_path(exchange_product_price_notice) %></td>
        <td><%= link_to 'Destroy', exchange_product_price_notice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exchange Product Price Notice', new_exchange_product_price_notice_path %>
